<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="autolayoutconstraints" value="false"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showconstraints" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="FOPGraphFramework">
			<and mandatory="true" name="Graph">
				<and mandatory="true" name="Edges">
					<feature name="Directed"/>
					<feature name="Weighted"/>
				</and>
				<or abstract="true" name="Algorithms">
					<feature name="DFS"/>
					<feature name="BFS"/>
					<feature name="MST"/>
					<feature name="Adjacency_Matrix"/>
				</or>
				<and mandatory="true" name="Nodes">
					<feature name="Color"/>
					<feature name="Labeled"/>
				</and>
			</and>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>BFS</var>
				<var>Adjacency_Matrix</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>MST</var>
				<conj>
					<var>Weighted</var>
					<not>
						<var>Directed</var>
					</not>
				</conj>
			</imp>
		</rule>
	</constraints>
	<featureOrder userDefined="true">
		<feature name="Graph"/>
		<feature name="Edges"/>
		<feature name="Nodes"/>
		<feature name="Color"/>
		<feature name="Labeled"/>
		<feature name="Weighted"/>
		<feature name="Adjacency_Matrix"/>
		<feature name="DFS"/>
		<feature name="BFS"/>
		<feature name="MST"/>
		<feature name="Directed"/>
	</featureOrder>
</featureModel>
